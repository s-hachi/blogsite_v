<!DOCTYPE html>
<html>
<head>
  <title>Blogsite</title>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <!-- Compiled and minified CSS -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">

  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>

  <script
  src="https://code.jquery.com/jquery-3.3.1.js"
  integrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60="
  crossorigin="anonymous"></script>

<nav>
<div class="nav-wrapper">
  <a href="/blogs" class="brand-logo">Blogsite</a>
  <% if user_signed_in? %>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li><%= link_to "Logout", destroy_user_session_path, method: :delete, class: 'btn' %></li>
        <li><a class="btn" href="/blogs/new">New</a></li>
      </ul>
    </div>
    <% else %>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li><%= link_to "Login", new_user_session_path, class: 'btn' %></li>
        <li><%= link_to "New", new_user_registration_path, class: 'btn' %></li>
      </ul>
    <% end %>


</div>
</nav>

</head>
<body>

<%= yield %>

</body>
</html>
